	.modal-dialog.modal-dialog-centered.video-filter-modal
		.modal-content
			.modal-body
				img(src="https://img.betanysports.eu/landing/videos/close.svg", alt="close" class='video-filter-modal__close-modal' data-bs-dismiss='modal')
